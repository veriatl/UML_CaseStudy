  UML!ConsiderIgnoreFragment.allInstances()->forAll(cif |  
cif.message->size()>0 implies 
    cif.message->forAll(nem|
      nem.oclIsKindOf(UML!Operation) or nem.oclIsKindOf(UML!Signal))
